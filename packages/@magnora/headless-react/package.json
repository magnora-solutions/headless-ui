{
  "name": "@magnora/headless-react",
  "version": "1.0.0",
  "description": "Headless React components for Shopify reviews and ratings with plug-and-play architecture",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rm -rf dist node_modules"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./shopify": {
      "types": "./dist/shopify.d.ts",
      "import": "./dist/shopify.esm.js",
      "require": "./dist/shopify.js"
    }
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@react-aria/interactions": "^3.15.0",
    "@react-aria/focus": "^3.12.0",
    "@react-aria/utils": "^3.17.0",
    "@react-types/shared": "^3.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.7",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.3.0"
  },
  "keywords": [
    "react",
    "headless",
    "components",
    "ui",
    "shopify",
    "reviews",
    "ratings",
    "accessibility",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/magnora/reviews",
    "directory": "packages/@magnora/headless-react"
  },
  "bugs": {
    "url": "https://github.com/magnora/reviews/issues"
  },
  "homepage": "https://github.com/magnora/reviews#readme",
  "author": "Magnora Team",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
} 